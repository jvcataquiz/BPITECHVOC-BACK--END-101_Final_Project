<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="/js/jquery.js"></script>
    <link rel="stylesheet" href="/style.css">
    <title>Landing Page</title>
</head>
<style>

</style>

<body class="bg-img">
   
    
<%- include('navbar') %> 
    <!-- maincontent -->
    <div class="posting">

        <% for(let post of file){ %>

            <div class="card">
                <div class="img"
                    style="background-image: url('<%=post.imgUrl%>');">

                </div>
                <div class="description">
                    <h2> â‚± <%= post.price %> </h2>
                    <h6 class="title"><%= post.fullname %> </h6>
                    <div class="address">
                        <p><%= post.address.slice(0, 50) %> ...</p>
                    </div>
                    <div class="short">
                        <strong>Description:</strong>
                        <p> <%= post.description.slice(0, 70) %> ... </p>
                    </div>
                    <div class="row">
                        <p class="dateposted"><%= post.date.toLocaleDateString()%>  </p>
                        <p class="title reviews"><%= post.comments.length %> Reviews </p>
                    </div>
                </div>
                <p>
                    
                            <a href="/rental/<%=post._id%>"   id="view" >View</a>

                          
                          
   
            </div>




            <% } %>


    </div>

    <div class="map">

    </div>



</body>

<script>
    function notsignin() {
            alert("Please sign in"); 
    }
    </script>
    
</html>